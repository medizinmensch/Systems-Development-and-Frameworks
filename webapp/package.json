{
  "name": "sdaf_todo_list",
  "description": "SDAF Todo list with vue cli, bootstrap, ESLint, Travis & Apollo GraphQL.",
  "version": "1.0.0",
  "author": "Youri Seichter <yseichter@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "./node_modules/.bin/cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "./node_modules/.bin/cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "./node_modules/.bin/jest",
    "lint": "./node_modules/.bin/eslint . \"**/*.vue\""
  },
  "dependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "apollo-server": "^2.9.9",
    "apollo-server-testing": "^2.9.9",
    "babel-eslint": "^10.0.3",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.0.0-beta.2",
    "bootstrap-vue": "^1.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-vue": "^5.2.3",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.6",
    "jest": "^24.9.0",
    "mock-apollo-client": "^0.2.0",
    "popper.js": "^1.12.9",
    "random-int": "^2.0.1",
    "uuid": "^3.3.3",
    "vue": "2.6.6",
    "vue-apollo": "^3.0.0",
    "vue-server-renderer": "2.6.6"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^6.0.3",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "jsdom": "^15.2.1",
    "jsdom-global": "^3.0.2",
    "style-loader": "^0.19.0",
    "vue-jest": "^3.0.5",
    "vue-loader": "^13.5.0",
    "vue-template-compiler": "2.6.6",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
